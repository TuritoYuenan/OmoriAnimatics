{
	"name": "omori-animatics",
	"author": "Turito Yuenan",
	"description": "Collection of Youtube's OMORI animatics",
	"version": "1.0.0",
	"type": "module",
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"singleQuote": true,
		"bracketSameLine": true,
		"trailingComma": "none",
		"quoteProps": "preserve",
		"plugins": [
			"prettier-plugin-svelte"
		],
		"overrides": [
			{
				"files": "*.svelte",
				"options": {
					"parser": "svelte"
				}
			}
		]
	}
}
